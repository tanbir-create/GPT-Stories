{
  "name": "gpt-stories",
  "version": "1.0.0",
  "description": "Share your AI stories ",
  "main": "index.js",
  "scripts": {
    "clean": "rmdir /s /q build && mkdir build",
    "build-server": "cross-env babel ./src --out-dir ./build --source-maps",
    "build": "npm run clean && npm run build-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "devOnce": "cross-env NODE_ENV=development && babel-node app.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/app.js",
    "prod-build": "cross-env NODE_ENV=production node ./build/app.js",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "keywords": [
    "Chat",
    "GPT",
    "Social",
    "media",
    "AI",
    "Bookmark",
    "Link"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
